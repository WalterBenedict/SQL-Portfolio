# I wrote the following code for this project

/* Items listed by price in descending order */
SELECT 
	item_name, 
	price 
FROM superstore 
ORDER BY price;


/* Sum of all money spent, highest&lowest individual expense, and average cost per item */
SELECT
	SUM(price) AS "Sum of all money spent", 
	MAX(price) AS "Highest individual expense", 
	MIN(price) AS "Lowest individual expense",
    AVG(price) AS "Average cost per item"
FROM superstore;


/* Price of all Kichen Supplies */
SELECT
	category, 
	SUM(price) AS "Total Price of Kitchen Supplies"
FROM superstore
WHERE category = "Kitchen Supplies";


SELECT
	category AS "Category", 
	SUM(price) AS "Total spent in each category"
FROM superstore
GROUP BY category;
