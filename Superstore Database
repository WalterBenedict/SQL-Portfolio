# This project utilizes Superstore data from the following training link:  https://www.db-fiddle.com/f/PvBAaQwEUSWAxZCsg4Vmx/0

CREATE TABLE superstore (
    item_id INTEGER PRIMARY KEY,
    item_name TEXT,
    category TEXT,
    price DECIMAL(10, 2),
    stock_quantity INTEGER,
    average_rating DECIMAL(3, 2)
);

